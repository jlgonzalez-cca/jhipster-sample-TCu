    <!--
        Added the entity ACCENTIDADDF.
    -->
    <changeSet id="20200408113300-1" author="jhipster">
        <createTable tableName="accentidaddf">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="i_dentidad" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="n_ombre" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="f_echaconstitucion" type="date">
                <constraints nullable="true" />
            </column>
            <column name="f_echaextincion" type="date">
                <constraints nullable="true" />
            </column>
            <column name="d_omicilio" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="c_odigopostal" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="t_elefono" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="e_mail" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="d_eu" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="f_echaaltaapp" type="date">
                <constraints nullable="true" />
            </column>
            <column name="f_echabajaapp" type="date">
                <constraints nullable="true" />
            </column>
            <column name="f_echaaltacenso" type="date">
                <constraints nullable="true" />
            </column>
            <column name="f_echabajacenso" type="date">
                <constraints nullable="true" />
            </column>
            <column name="n_umeroversion" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="f_echaversion" type="date">
                <constraints nullable="true" />
            </column>
            <column name="identidadfk1_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="identidadfk2_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
    </changeSet>

    <changeSet id="20200408113300-1-relations" author="jhipster">

    </changeSet>
    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here, do not remove-->

