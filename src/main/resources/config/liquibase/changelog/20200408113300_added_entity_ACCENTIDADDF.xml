            <column name="f_echaconstitucion" type="date"/>
            <column name="f_echaextincion" type="date"/>
            <column name="d_omicilio" type="string"/>
            <column name="c_odigopostal" type="string"/>
            <column name="t_elefono" type="string"/>
            <column name="e_mail" type="string"/>
            <column name="d_eu" type="string"/>
            <column name="f_echaaltaapp" type="date"/>
            <column name="f_echabajaapp" type="date"/>
            <column name="f_echaaltacenso" type="date"/>
