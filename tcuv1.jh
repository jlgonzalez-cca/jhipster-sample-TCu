entity ACCENTIDADPER {
 // IDENTIDAD	Long,
 // IDCARGO	Long,
  NIF	String,
  NOMBRE	String,
  APELLIDO1	String,
  APELLIDO2	String,
EMAIL	String
}

// an ignored comment
/** not an ignored comment */
entity DCCCARGOENT {
	IDCARGO	Long,
	NOMBRE	String
}

relationship OneToMany {
	ACCENTIDADDVT to ACCENTIDADDF{IDENTIDAD},
	ACCENTIDADPER to ACCENTIDADDF{IDENTIDAD},
	ACCENTIDADPER to DCCCARGOENT{IDCARGO}
}

// Set pagination options
paginate DCCCARGOENT with infinite-scroll
paginate ACCENTIDADPER,ACCENTIDADDF,ACCENTIDADDVT  with pagination

